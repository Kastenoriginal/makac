<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sk.tuke.smart.makac.WorkoutDetailActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/button_workoutdetail_showmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MAP"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/textview_workoutdetail_valuedistance"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textview_workoutdetail_workouttitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Workout Details"
        android:textSize="36sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textview_workoutdetail_labelsport"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sport"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        android:layout_marginStart="16dp"
        android:layout_marginTop="100dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="99dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textview_workoutdetail_sportactivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Running"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/textview_workoutdetail_labelsport"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textview_workoutdetail_labelsport" />

    <TextView
        android:id="@+id/textview_workoutdetail_activitydate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="07.10.2017"
        android:textSize="24sp"
        android:layout_marginStart="34dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textview_workoutdetail_sportactivity"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textview_workoutdetail_sportactivity" />

    <TextView
        android:id="@+id/textview_workoutdetail_labelsummary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Summary"
        android:textSize="18sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="172dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Duration"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/textview_workoutdetail_labelsummary"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textview_workoutdetail_valueduration" />

    <TextView
        android:id="@+id/textview_workoutdetail_valueduration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00:00:00"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textview_workoutdetail_valuedistance"
        android:layout_marginTop="11dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/textview_workoutdetail_valuedistance" />

    <TextView
        android:id="@+id/textview_workoutdetail_labelcalories"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calories"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="22dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calories"
        android:textSize="24sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textview_workoutdetail_labelavgpace"
        app:layout_constraintRight_toRightOf="@+id/textview_workoutdetail_valueavgpace"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="21dp"
        app:layout_constraintLeft_toRightOf="@+id/textview_workoutdetail_valueavgpace" />

    <TextView
        android:id="@+id/textview_workoutdetail_labeldistance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distance"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/textview_workoutdetail_valueduration"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textview_workoutdetail_valuedistance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0.00"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/textview_workoutdetail_labeldistance"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textview_workoutdetail_labeldistance" />

    <TextView
        android:id="@+id/textview_workoutdetail_labelavgpace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pace"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textview_workoutdetail_valueavgpace"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textview_workoutdetail_valueavgpace" />

    <TextView
        android:id="@+id/textview_workoutdetail_valueavgpace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pace"
        android:textSize="24sp"
        app:layout_constraintRight_toLeftOf="@+id/button_workoutdetail_fbsharebtn"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="19dp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_workoutdetail_valuedistance"
        android:layout_marginEnd="18dp"
        app:layout_constraintTop_toBottomOf="@+id/textview_workoutdetail_labeldistance"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/button_workoutdetail_emailshare" />

    <TextView
        android:id="@+id/textview_workoutdetail_labelsocialshare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share"
        android:textSize="18sp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button_workoutdetail_gplusshare"
        android:layout_marginStart="11dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="2dp"
        app:layout_constraintLeft_toLeftOf="@+id/button_workoutdetail_gplusshare" />

    <Button
        android:id="@+id/button_workoutdetail_gplusshare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="G+"
        android:layout_marginStart="5dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/button_workoutdetail_twittershare"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button_workoutdetail_twittershare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tweet"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toRightOf="@+id/button_workoutdetail_gplusshare" />

    <Button
        android:id="@+id/button_workoutdetail_emailshare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="email"
        android:layout_marginStart="1dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/button_workoutdetail_twittershare"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/button_workoutdetail_twittershare" />

    <Button
        android:id="@+id/button_workoutdetail_fbsharebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FB"
        android:layout_marginStart="2dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/button_workoutdetail_emailshare"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/button_workoutdetail_emailshare" />
</android.support.constraint.ConstraintLayout>
